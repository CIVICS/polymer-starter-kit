{
  "name": "polymer-starter-kit",
  "version": "0.0.0",
  "dependencies": {},
  "scripts": {
    "serve": "./node_modules/.bin/gulp serve",
    "serve:dist": "./node_modules/.bin/gulp serve:dist",
    "build:unvulcanized": "./node_modules/.bin/gulp",
